{
  "name": "mxaws",
  "version": "1.0.0",
  "description": "A collection of wrappers simplifying calls from the AWS JS SDK",
  "main": "./dist/mxaws.js",
  "scripts": {
    "build": "babel src -d dist",
    "prepare": "npm run build",
    "clean": "rm -rf ./node_modules"
  },
  "author": "Frank Camilleri",
  "dependencies": {
    "aws-sdk": "^2.92.0",
    "babel-polyfill": "^6.26.0",
    "nconf": "^0.8.4",
    "netcat": "^1.3.2"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "parserOpts": {
      "allowReturnOutsideFunction": true
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  }
}
